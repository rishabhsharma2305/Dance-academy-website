doctype html
html
    head
        style
            include ../static/style.css
            include ../static/stylecontact.css
    body
        nav#navbar
            ul
                div#logo
                    img(src="../static/dnimg1.png")
                li #[a(href="/s") Home]
                li #[a(href="/s") About]
                li #[a(href="/s") services]
                li #[a(href="/s") Contact]
        h3 Please fill this form
        form(action="/contact",method="post", class="myform")
            input(type="text", class="myinput", name="name", placeholder="Your name")
            input(type="text", class="myinput", name="phone", placeholder="Your phone")
            input(type="text", class="myinput", name="email", placeholder="Your email")
            button(class="btn")
               | submit
            